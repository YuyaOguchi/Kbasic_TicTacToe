/*
when click on the cmd player1,
add counter by 1.
if you get to 5, then display you win and reset to both player count to 0
display the number on the lblplayers
repeat same with player 2
*/

dim myCounter1 as integer



Private Sub cmdplayer1_OnEvent()
  myCounter1 = myCounter1 + 1
  lblplayer1.caption = myCounter1
  
  if myCounter1 > 4 then
    msgbox(myname1 & " Wins!")
    myCounter1 = 0
    mycounter2 = 0
  lblplayer1.caption = myCounter1
  lblplayer2.caption = myCounter2
  cmd1.visible = true
  cmd2.visible = true
  cmd3.visible = true
  cmd4.visible = true
  cmd5.visible = true
  cmd6.visible = true
  cmd7.visible = true
  cmd8.visible = true
  cmd9.visible = true
  endif
End Sub

dim mycounter2 as integer

Private Sub cmdplayer2_OnEvent()
  myCounter2 = myCounter2 + 1
