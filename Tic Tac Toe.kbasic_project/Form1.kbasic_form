  else
    Label1.caption="O"
    X_Turn=true
  end if
End Sub
*/



Dim X_Turn as boolean
dim myname1 as string
dim myname2 as string

/*
MAKE SURE THAT YOU CAN NOT PLAY THE GAME UNTIL THE PERSON CLICK ON "PLAY"
Type in player names in the input box to show it on cmdplayers 
*/

Private Sub cmdstart_OnEvent()
  lblclickstart.visible = False
  myname1 = inputbox("what is first player's name?")
  cmdplayer1.caption = myname1 & " (O) " 
  myname2 = inputbox("What is second player's name?")
  cmdplayer2.caption = myname2 & " (X) "
  msgbox "Hello " & myname1 & " and " & myname2 & " lets play Tic Tac Toe!"
 cmd1.enabled = true
 cmd2.enabled = true
 cmd3.enabled = true
 cmd4.enabled = true
 cmd5.enabled = true
 cmd6.enabled = true
 cmd7.enabled = true
 cmd8.enabled = true
 cmd9.enabled = true
 lbl1.enabled = true
 lbl2.enabled = true
 lbl3.enabled = true
 lbl4.enabled = true
 lbl5.enabled = true
 lbl6.enabled = true
 lbl7.enabled = true
 lbl8.enabled = true
 lbl9.enabled = true
 /*
  end if
 */ 
End Sub



Private Sub cmdquit_OnEvent()
  end
End Sub

/*
reset = make all the turned off cmd of the squares to 
visible so that you can play next game by clicking on it
*/

Private Sub cmdreset_OnEvent()
  cmd1.visible = true
  cmd2.visible = true
  cmd3.visible = true
  cmd4.visible = true
  cmd5.visible = true
  cmd6.visible = true
  cmd7.visible = true
  cmd8.visible = true
  cmd9.visible = true
   
End Sub


/*
display the help when clicked
*/

Private Sub cmdhelp_OnEvent()
  msgbox "Click on start to start the game. Click on a player to add a point."
  msgbox "Your objective is to make vertical or horizontal line of three  of your figures figures. Player1 plays circle (o) and Player2 plays cross (X). Player1 will always go first on the first round and it will alternate every turn.If you win the match, click your name on the bottom of your scrren and you will gain a point. If its a draw, press reset and you may begin your next game. First one to reach 5 points wins the game."
End Sub

