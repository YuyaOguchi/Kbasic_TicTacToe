    msgbox(myname2 & " Wins!")
    mycounter1 = 0
    myCounter2 = 0
    lblplayer1.caption = myCounter1
    lblplayer2.caption = myCounter2
    cmd1.visible = true
  cmd2.visible = true
  cmd3.visible = true
  cmd4.visible = true
  cmd5.visible = true
  cmd6.visible = true
  cmd7.visible = true
  cmd8.visible = true
  cmd9.visible = true
  endif
End Sub



